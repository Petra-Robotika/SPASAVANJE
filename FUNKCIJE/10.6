#pragma once
#include "helper.h"

void RobotLine::radionica()
{
  armOpen();
wallFollowRight();
   if (leftFront() < 120) {
    go(50, 50);
    delayMs(600);
    go(-90, 90);
    delayMs(500);
    go(50, 50);
    delayMs(1900);
    armClose();
    go(-90, -90);
    delayMs(500);
    go(-90, 90)
    delayMs(500);
    go(50, 50);
    if (lineAny()) {
    stop();
    end();

    
  }
   if (front() < 120) {
    go(-90, 90);
    delayMs(500);
   }

  }
